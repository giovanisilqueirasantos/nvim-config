{
  "configurations": {
    "run_go": {
      "adapter": "delve",
      "filetypes": [ "go" ],
      "variables": {},
      "configuration": {
        "request": "launch",
        "program": "${fileDirname}",
        "mode": "debug"
      }
    },
    "run_go_test": {
      "adapter": "delve",
      "filetypes": [ "go" ],
      "variables": {},
      "configuration": {
        "request": "launch",
        "program": "${fileDirname}",
        "mode": "test"
      }
    },
    "run_python": {
      "adapter": "debugpy",
      "filetypes": [ "python" ],
      "default": true,
      "configuration": {
        "name": "python debug",
        "type": "python",
        "request": "launch",
        "python": "/usr/bin/python3",
        "stopOnEntry": true,
        "console": "internalConsole",
        "debugOptions": [],
        "program": "${file}"
      }
    }
  }
}
